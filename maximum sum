import java.util.Arrays;
public class Main {
    public static void main(String[] args) {
        int[] a = {4, 7, 8, 2, 3, 1};
        int n = a.length;
        for (int i = 0; i < n - 1; i++) {
            if (a[i + 1] < a[i]) {
                a[i] = a[i + 1];
            } else {
                a[i] = -1;
            }
        }
        System.out.println(Arrays.toString(a));
    }
}
